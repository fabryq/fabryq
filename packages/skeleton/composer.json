{
  "name": "fabryq/skeleton",
  "description": "Fabryq project skeleton",
  "type": "project",
  "license": "MIT",
  "version": "0.1.0",
  "require": {
    "php": ">=8.2",
    "symfony/framework-bundle": "^7.0",
    "symfony/console": "^7.0",
    "symfony/yaml": "^7.0",
    "symfony/twig-bundle": "^7.0",
    "symfony/dotenv": "^7.0",
    "doctrine/orm": "^2.17",
    "doctrine/doctrine-bundle": "^2.11",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "zircote/swagger-php": "^5.7.6",
    "fabryq/runtime": "^0.3",
    "fabryq/cli": "^0.3",
    "fabryq/provider-http-client": "^0.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/Apps/",
      "App\\Components\\": "src/Components/"
    },
    "classmap": [
      "src/Kernel.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "@php bin/console fabryq:assets:install"
    ],
    "post-update-cmd": [
      "@php bin/console fabryq:assets:install"
    ]
  }
}
