{
  "name": "fabryq/examples-demo",
  "description": "Fabryq demo project",
  "type": "project",
  "license": "MIT",
  "version": "0.1.0",
  "require": {
    "php": ">=8.2",
    "fabryq/contracts": "^0.1",
    "fabryq/runtime": "^0.1",
    "fabryq/cli": "^0.1",
    "fabryq/provider-http-client": "^0.1",
    "doctrine/doctrine-bundle": "^2.11",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^2.17",
    "symfony/console": "^7.0",
    "symfony/dotenv": "^7.0",
    "symfony/framework-bundle": "^7.0",
    "symfony/twig-bundle": "^7.0",
    "symfony/webpack-encore-bundle": "^2.0",
    "symfony/yaml": "^7.0"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../packages/contracts",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "../../packages/runtime",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "../../packages/cli",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "../../packages/provider-http-client",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "../../skeleton",
      "options": {
        "symlink": true
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/Apps/",
      "App\\Components\\": "src/Components/"
    },
    "classmap": [
      "src/Kernel.php"
    ]
  },
  "require-dev": {
    "symfony/process": "^7.0"
  }
}
